<apex:page title="CGPH Design" standardStylesheets="false">
<style>
    @import url('https://fonts.googleapis.com/css2?family=Roboto Mono');
    body  {font-family: Roboto Mono}
    table {border-collapse:collapse}
    th    {font-size: 10pt; padding: 5px 5px 5px 5px; font-family: Roboto Mono;}
    td    {font-size: 10pt; padding: 5px 5px 5px 5px; font-family: Roboto Mono;}
    button { background-color: white; opacity: 100%; border: 0px; border-radius: 2px;}
    button:hover { opacity: 60%;}
    button:active { background:RGB(3,45,96); }
    .center {text-align:center}
    .top {font-size: 20pt; padding: 10px 0px 20px 0px; font-family: Roboto Mono; color: RGB(0,101,168); text-align: center;}
    .icon {opacity: 50%}
    .icon:hover {opacity:100%}
    
</style>

